programa {
  funcao inicio() {
    inteiro n1, n2, n3

   escreva("Digite um número: ")
   leia(n1)
   escreva("Digite outro número: ")
   leia(n2)
   escreva("Digite mais um número: ")
   leia(n3)

   se (n1 > n2 e n1 > n3)
   escreva("O maior número é ",n1)

   se (n2 > n1 e n2 > n3)
   escreva("O maior número é ",n2)

   se (n3 > n2 e n3 > n1)
   escreva("O maior número é ",n3)
  }
  }

